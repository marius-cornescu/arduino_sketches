//= VARIABLE =================================================================================================
int RELAY_CONTROL_PIN = 8;


//==================================================================================================
void setup() {
  pinMode(RELAY_CONTROL_PIN, OUTPUT);
}
//==================================================================================================
void loop() {
  digitalWrite(RELAY_CONTROL_PIN, HIGH);
  delay(1000);                      // wait for a second
  //
  digitalWrite(RELAY_CONTROL_PIN, LOW);
  // Serial.print(c);

  delay(9000);                      // wait for a second
}
